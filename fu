[rewrite_local]
^https?:\/\/(www\.)?youtubei\.googleapis\.com\/youtubei\/v1\/player\?.* url jsonjq-response-body ".adPlacements?= [] | .instreamAds?= [] | .playerAds?= [] | .companionAds?= [] | .adBreaks?= [] | .adSlots?= [] | .bannerAds?= [] | .adBreakHeartbeat?= [] | .promotedContent?= []"

# next / watch_next / continuation （推荐流中的推广与插入）
^https?:\/\/(www\.)?youtubei\.googleapis\.com\/youtubei\/v1\/next\?.* url jsonjq-response-body ".ads?= [] | .playerAds?= [] | .engagementPanelContents?= [] | .promotedContent?= [] | .adPlacements?= []"

# browse / home / section list (首页推荐中的 promotedContent)
^https?:\/\/(www\.)?youtubei\.googleapis\.com\/youtubei\/v1\/browse\?.* url jsonjq-response-body ".promotedContent?= [] | .ads?= [] | .contents?= map(select(.ad == null))"

# search（搜索结果中的 promotedContent / 广告项）
^https?:\/\/(www\.)?youtubei\.googleapis\.com\/youtubei\/v1\/search\?.* url jsonjq-response-body ".ads?= [] | .contents?= map(select(.ad == null)) | .promotedContent?= []"

# reel / shorts（短视频流中的广告项）
^https?:\/\/(www\.)?youtubei\.googleapis\.com\/youtubei\/v1\/reel\/.* url jsonjq-response-body ".ads?= [] | .promotedContent?= []"

# watch 页面（old watch endpoints / get_video_info）
^https?:\/\/.*\/get_video_info\?.* url jsonjq-response-body ".adformat?= null | .ad_status?= null | .ads?= []"

# 强制拒绝明显的广告端点（pagead / ptracking / get_midroll_ 等）
^https?:\/\/(www\.)?youtube\.com\/api\/stats\/ads url reject
^https?:\/\/(www\.)?youtube\.com\/get_midroll_ url reject
^https?:\/\/(www\.)?youtube\.com\/pagead\/ url reject
^https?:\/\/(www\.)?youtube\.com\/ptracking url reject
^https?:\/\/(www\.)?youtube\.com\/.*\/midroll.* url reject

# 拒绝 Google 的广告媒体或带广告参数的视频片段请求（若出现广告相关 query 参数）
^https?:\/\/.*\.googlevideo\.com\/.*[?&](oad|adformat|ad_type|ctier|ctier=A).* url reject
^https?:\/\/.*doubleclick\.net\/.* url reject
^https?:\/\/.*googlesyndication\.com\/.* url reject
^https?:\/\/.*adservice\.google\..* url reject


